{
    "name": "pubsub-ui",
    "version": "1.0.0",
    "description": "Streamlit UI for Google Cloud Pub/Sub emulator with AI features",
    "type": "command",
    "command": {
        "run": "streamlit run ./src/app.py",
        "requirements": "requirements.txt"
    },
    "env": {
        "PUBSUB_EMULATOR_HOST": "localhost:8681"
    }
}
