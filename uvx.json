{
    "name": "pubsub-emulator-ui",
    "version": "1.0.0",
    "description": "Streamlit UI for Google Cloud Pub/Sub emulator with AI features",
    "main": "app.py",
    "type": "streamlit",
    "dependencies": {
        "python": "requirements.txt",
        "docker": {
            "services": ["pubsub_emulator"],
            "file": "docker-compose.yml"
        }
    },
    "scripts": {
        "start": "streamlit run app.py",
        "docker": "docker-compose up -d"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/pubsub-emulator-ui.git"
    }
}
